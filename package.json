{
  "name": "cosmic-blog",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "npm run clean; ./node_modules/.bin/webpack -p; npm run build:server; NODE_ENV=production npm run serve",
    "start:server": "./node_modules/nodemon/bin/nodemon.js lib/app.js --exec babel-node --presets env",
    "start:client": "./node_modules/.bin/webpack-dev-server --port=3000 --inline",
    "serve": "node ./dist/app.js",
    "clean": "rm -rf dist && mkdir dist",
    "test": "mocha --compilers js:babel-core/register --require ./app/src/test/helpers --require ./app/src/test/helpers/enzyme.js  ./app/src/test/*.spec.js",
    "build:server": "babel lib -d dist",
    "build:client": "./node_modules/.bin/webpack -d",
    "build": "npm-run-all build:*",
    "production": "npm-run-all build:* && NODE_ENV=production node ./dist/app.js;"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cosmicjs": "2.4.22",
    "express": "4.16.2",
    "history": "4.7.2",
    "jsdom": "^11.5.1",
    "lodash": "4.17.4",
    "moment": "2.29.4",
    "node-sass": "4.7.2",
    "prop-types": "15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.14.7",
    "npm-run-all": "^4.1.2",
    "sass-loader": "6.0.6",
    "sinon": "^4.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
